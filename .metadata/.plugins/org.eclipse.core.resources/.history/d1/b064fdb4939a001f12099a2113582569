package chapter07;

public class LocalValEx
{

	public static void main(String[] args)
	{
		Local local = new Local();
		
		System.out.println(local.name); // null
		
		local.process();
		System.out.println(local.name); // 김기동
		
		local.
	}

}

class Local {
	String name;
	
	void process() {
		name = "김기동";
	}
	
	void printAge1() {
		int age = 20; // 지역 변수
	}
}