package chapter03;

public class FloatDoubleEx
{

	public static void main(String[] args)
	{
		float f = 3.14159f;
		double d = 3.14159d;
		
		System.out.println("float : " + f);
		System.out.println("double : " + d);
		
		float f2 = 3.14159;
		double d2 = 3.14159;
		System.out.println("double : " + f2);
		System.out.println("double : " + d2);
	}

}
