
public class QuUpgradeGuGu
{

	public static void main(String[] args)
	{
		for(int dan=2; dan<=9; dan++)
		{
			long result = dan;
			System.out.println(dan + " x 1 = " + dan);
			
			for(int su=2; su<=9; su++)
			{
				result *= dan;
				System.out.print(dan);
				
				for(int ex=1; ex<su; ex++)
				{
					System.out.print(dan + " x ");
				}
				
				System.out.println(dan + " = " + result);
			}
			
			System.out.println();
		}
	}

}

