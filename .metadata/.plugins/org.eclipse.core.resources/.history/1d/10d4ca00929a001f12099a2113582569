package chapter07;

public class VarEx
{

	public static void main(String[] args)
	{
		// 클래스 변수 사용.
		System.out.println("Morning 제조사 : " + Morning.company);
		
		Morning m1 = new Morning();
		Morning m2 = new Morning();
	}

}

class Morning {
	String color; // 인스턴스 변수.
	static String company = "기아"; // 클래스 변수.
}