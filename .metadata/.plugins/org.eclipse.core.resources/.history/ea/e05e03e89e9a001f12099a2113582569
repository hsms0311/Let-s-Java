package chapter04;

public class OpEx1 {

	public static void main(String[] args) {
		
		int number1 = 10;
		System.out.println("number1 = 10 -> " + number1);

		number1 += 10;
		System.out.println("number1 += 10 -> " + number1);

		number1 -= 10;
		System.out.println("number1 -= 10 -> " + number1);

		number1 *= 5;
		System.out.println("number1 *= 5 -> " + number1);

		number1 /= 10;
		System.out.println("number1 /= 10 -> " + number1);

		number1 %= 2;
		System.out.println("number1 %= 2 -> " + number1);
	}

}
