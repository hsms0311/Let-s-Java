package chapter07;

public class InitEx2 {
	// static 변수
	static int sVar;
	// static 메서드
	static void sMethod() {
		
	}

	// 인스턴스 변수
	int var;
	// 인스턴스 메서드
	void method() {
		
	}
	
	// static 초기화 블럭
	static {
		sVar = 0; // static 변수
		sMethod(); // static 메서드
		
		// 에러(인스턴스 변수, 메서드 사용 불가)
		var = 0; // 인스턴스 변수
		method(); // 인스턴스 메서드
	}
	
	public static void main(String[] args) {
		System.out.println("main 메서드 시작");
		System.out.println("main init1 객체 생성");
		InitEx2 init1 = new InitEx2();
		System.out.println("main init2 객체 생성");
		InitEx2 init2 = new InitEx2();
	}

}
